!URxvt.perl-ext-common: default,clipboard,selection-to-clipboard,matcher,resize-font
!URxvt.perl-ext-common: default,matcher,resize-font,tabbedex
URxvt.perl-ext-common: default,selection-to-clipboard,matcher,tabbedex,font-size
!URxvt.perl-ext-common: default,tabbed,matcher,resize-font

! font
URxvt*font: xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true,xft:Liberation Mono:pixelsize=13:antialias=true:hinting=true


Xft.autohint:   0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    true
Xft.antialias:  true
Xft.rgba:       rgb

! terminal padding
URxvt.internalBorder: 25
URxvt.intensityStyles: false
URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1


! colors
*.cursorColor:  #ebdbb2
*.background: #282828
*.foreground: #ebdbb2

! black
*.color0:       #282828
*.color8:       #928374

! red
*.color1:       #cc241d
*.color9:       #fb4934

! green
